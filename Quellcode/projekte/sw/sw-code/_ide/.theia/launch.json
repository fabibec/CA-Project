{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "tcf-debug",
      "request": "launch",
      "name": "sw-code_app_hw_1",
      "debugType": "baremetal-fpga",
      "attachToRunningTargetOptions": {
        "targetSetupMode": "standalone",
        "executeScript": true,
        "scriptPath": ""
      },
      "autoAttachProcessChildren": false,
      "target": {
        "targetConnectionId": "Local",
        "peersIniPath": "../../../.peers.ini",
        "context": "fpga"
      },
      "targetSetup": {
        "resetSystem": true,
        "programDevice": true,
        "bitstreamFile": "${workspaceFolder}/sw-code/_ide/bitstream/hw-platform.bit",
        "downloadElf": [
          {
            "core": "microblaze_0",
            "resetProcessor": true,
            "elfFile": "${workspaceFolder}/sw-code/build/sw-code.elf",
            "stopAtEntry": false
          }
        ]
      },
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}